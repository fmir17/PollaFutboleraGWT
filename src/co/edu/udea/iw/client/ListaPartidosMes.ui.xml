<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	
	<g:HTMLPanel>
		<center>
			<g:Label horizontalAlignment="center" styleName="labelMensaje"
				visible="false" ui:field="labelMensaje"/>
			<g:VerticalPanel spacing="5">
				<g:Label horizontalAlignment="center">Lista de partidos</g:Label>
				<g:FlexTable ui:field="tabla" width="400px" borderWidth="1"
					cellPadding="0" cellSpacing="0" styleName="tabla"/>
				
				<g:Button text="Agregar" ui:field="agregar"></g:Button>
			</g:VerticalPanel>
		</center>
	</g:HTMLPanel>
	
	
</ui:UiBinder> 